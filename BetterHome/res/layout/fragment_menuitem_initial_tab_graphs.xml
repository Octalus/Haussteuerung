<?xml version="1.0" encoding="UTF-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <TableRow android:id="@+id/tableRow1" >

        <LinearLayout
		   android:id="@+id/graph1"
		   android:orientation="vertical"
		   android:gravity="end"
		   android:layout_width="400dp"
           android:layout_height="335dp"
           android:paddingRight="5dp"
		   android:layout_weight="1" >
			
            <LinearLayout
			   android:orientation="horizontal"
			   android:layout_width="match_parent"
	           android:layout_height="25dp"
	           android:gravity="end" >
	        	
	        	<ImageView android:id="@+id/openButton1"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_full_screen"/>
	        	
	        	<ImageView android:id="@+id/delButton1"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_remove"/>
	        	
	        </LinearLayout>
            
        </LinearLayout>

        <LinearLayout
		   android:id="@+id/graph2"
		   android:orientation="vertical"
		   android:gravity="end"
		   android:layout_width="400dp"
           android:layout_height="335dp"
           android:paddingLeft="5dp"
		   android:layout_weight="1" >
        	
            <LinearLayout
			   android:orientation="horizontal"
			   android:layout_width="match_parent"
	           android:layout_height="25dp"
	           android:gravity="end" >
	        	
	        	<ImageView android:id="@+id/openButton2"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_full_screen"/>
	        	
	        	<ImageView android:id="@+id/delButton2"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_remove"/>
	        	
	        </LinearLayout>
	            
        </LinearLayout>
    </TableRow>

    <TableRow android:id="@+id/tableRow2" >

        <LinearLayout
		   android:id="@+id/graph3"
		   android:orientation="vertical"
		   android:gravity="end"
		   android:layout_width="400dp"
           android:layout_height="335dp"
           android:paddingRight="5dp"
		   android:layout_weight="2" >
        	
            <LinearLayout
			   android:orientation="horizontal"
			   android:layout_width="match_parent"
	           android:layout_height="25dp"
	           android:gravity="end" >
	        	
	        	<ImageView android:id="@+id/openButton3"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_full_screen"/>
	        	
	        	<ImageView android:id="@+id/delButton3"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_remove"/>
	        	
	        </LinearLayout>
            
        </LinearLayout>

        <LinearLayout
		   android:id="@+id/graph4"
		   android:orientation="vertical"
		   android:gravity="end"
		   android:layout_width="400dp"
           android:layout_height="335dp"
           android:paddingRight="5dp"
		   android:layout_weight="2" >
            
            <LinearLayout
			   android:orientation="horizontal"
			   android:layout_width="match_parent"
	           android:layout_height="25dp"
	           android:gravity="end" >
	        	
	        	<ImageView android:id="@+id/openButton4"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_full_screen"/>
	        	
	        	<ImageView android:id="@+id/delButton4"
	                android:layout_width="25dp"
	           		android:layout_height="25dp"
	           		android:src="@+drawable/ic_action_remove"/>
	        	
	        </LinearLayout>
            
        </LinearLayout>
    </TableRow>

</TableLayout>